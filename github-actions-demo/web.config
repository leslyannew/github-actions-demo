<?xml version="1.0" encoding="utf-8"?>
<!-- To customize the asp.net core module uncomment and edit the following section. 
  For more info see https://go.microsoft.com/fwlink/?linkid=838655 -->
<configuration>
  <location>
    <system.webServer>
      <handlers>
        <remove name="aspNetCore"/>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified"/>
      </handlers>
      <security>
        <requestFiltering>
          <requestLimits maxQueryString="8192" maxAllowedContentLength="1073741824"  />
        </requestFiltering>
      </security>
      <aspNetCore processPath="dotnet" arguments=".\github_actions_demo.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="InProcess">
      </aspNetCore>
    </system.webServer>
    <system.web>
      <httpRuntime maxRequestLength="2097151" executionTimeout="2097151" />
    </system.web>
  </location>
</configuration>
